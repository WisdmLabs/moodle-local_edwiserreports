<div class="col-md-4">
    <label>Cohorts</label>
    <select id="ed_cohorts" class="js-states form-control select25">
        <option value="0">All</option>
        {{#cohorts}}
        <option value="{{id}}">{{name}}</option>
        {{/cohorts}}
    </select>
</div>

<div class="col-md-4">
    <label>Users</label>
    <select id="ed_users" class="js-states form-control select25">
        <option value="0">All</option>
        {{#users}}
        <option value={{id}}>{{name}}</option>
        {{/users}}
    </select>
</div>