<div class="table-reponsive overflow-scroll">
    <table id="f2fsessionstable" class="table border-bottom-0">
        <thead class="bg-secondary">
            <tr>
                <th class="bg-secondary border-bottom-0">{{#str}} name, report_elucidsitereport{{/str}}</th>
                <th class="bg-secondary border-bottom-0">{{#str}} course, report_elucidsitereport {{/str}}</th>
                <th class="bg-secondary border-bottom-0">{{#str}} signups, report_elucidsitereport {{/str}}</th>
                <th class="bg-secondary border-bottom-0">{{#str}} attendees,report_elucidsitereport {{/str}}</th>
            </tr>
        </thead>
        <tbody>
            {{#f2fmodules}}
            <tr data-toggle="collapse" data-target="#accordion-{{id}}" class="clickable">
                <td>
                    {{name}}
                </td>
                <td>
                    {{coursename}}
                </td>
                <td>
                    {{overallsignups}}
                </td>
                <td data-value="1" class="footable-last-column">
                    {{overallattendend}}
                    <i class="icon accordion-arrow pull-right fa fa-angle-left text-center"></i>
                </td>
            </tr>
            <tr>
                <td class="border-0 py-0 pl-30" colspan="4">
                    <div  id="accordion-{{id}}" class="collapse">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th class="bg-secondary bg-image-none no-sorting-arrow border-bottom-0">{{#str}} date, report_elucidsitereport {{/str}}</th>
                                    <th class="bg-secondary bg-image-none no-sorting-arrow border-bottom-0">{{#str}} time, report_elucidsitereport {{/str}}</th>
                                    <th class="bg-secondary bg-image-none no-sorting-arrow border-bottom-0">{{#str}} venue, report_elucidsitereport {{/str}}</th>
                                    <th class="bg-secondary bg-image-none no-sorting-arrow border-bottom-0">{{#str}} signups, report_elucidsitereport {{/str}}</th>
                                    <th class="bg-secondary bg-image-none no-sorting-arrow border-bottom-0">{{#str}} attendees, report_elucidsitereport {{/str}}</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#sessions}}
                                <tr>
                                    <td>{{date}}</td>
                                    <td>{{time}}</td>
                                    <td>{{signupvenue}}</td>
                                    <td>{{signups}}</td>
                                    <td>{{attendend}}</td>
                                </tr>
                                {{/sessions}}
                                {{^sessions}}
                                    <tr>
                                        <td class="text-center" colspan="5">
                                            {{#str}} nof2fsession, report_elucidsitereport {{/str}}
                                        </td>
                                    </tr>
                                {{/sessions}}
                            </tbody>
                        </table>
                    </div>
                </td>
            </tr>
            {{/f2fmodules}}
            {{^f2fmodules}}
            <tr>
                <td class="text-center" colspan="4">{{#str}} nof2fmodule, report_elucidsitereport {{/str}}</td>
            </tr>
            {{/f2fmodules}}
        </tbody>
    </table>
</div>