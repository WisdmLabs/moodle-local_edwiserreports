{{#exportlink}}
    {{> report_elucidsitereport/exportdropdown }}
{{/exportlink}}
{{#userfilters}}
    {{> report_elucidsitereport/userfilters }}
{{/userfilters}}

<div id="wdm-f2fsessions-individual" data-sesskey="{{sesskey}}">
    <div class="table-reponsive overflow-scroll">
        <div class="loader w-full text-center py-100 my-50">
            {{# pix }} loader, report_elucidsitereport{{/ pix }}
        </div>
        <table id="f2fsessionstable" class="table dataTable no-footer clickable" style="display: none;">
            <thead>
                <tr>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} date, report_elucidsitereport {{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} time, report_elucidsitereport {{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} name, report_elucidsitereport{{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} course, report_elucidsitereport {{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} venue, report_elucidsitereport {{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} signups, report_elucidsitereport {{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} confirmed, report_elucidsitereport {{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} attendees, report_elucidsitereport {{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} waitlist, report_elucidsitereport {{/str}}
                    </th>
                    <th class="text-nowrap bg-image-none no-sorting-arrow">
                        {{#str}} declined, report_elucidsitereport {{/str}}
                    </th>
                </tr>
            </thead>
            <tbody>
                {{#f2fmodules}}
                {{#sessions}}
                <tr class="text-nowrap" data-toggle="collapse" data-target="#accordion-{{sessionid}}" class="clickable">
                    <td>{{date}}</td>
                    <td>{{time}}</td>
                    <td>{{name}}</td>
                    <td>{{coursename}}</td>
                    <td>{{signupvenue}}</td>
                    <td class="text-center">{{signups}}</td>
                    <td class="text-center">{{confirmed}}</td>
                    <td class="text-center">{{attendend}}</td>
                    <td class="text-center">{{waitlisted}}</td>
                    <td class="text-center">
                        <i class="icon accordion-arrow pull-right fa fa-angle-left my-5"></i>
                        {{declined}}
                    </td>
                </tr>
                <tr>
                    <td class="border-0 collapse-td py-0 pl-30" colspan="10">
                        <div  id="accordion-{{sessionid}}" class="collapse">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th class="text-nowrap">
                                            {{#str}} username, report_elucidsitereport {{/str}}
                                        </th>
                                        <th class="text-nowrap">
                                            {{#str}} status, report_elucidsitereport {{/str}}
                                        </th>
                                        <th class="text-nowrap">
                                            {{#str}} reason, report_elucidsitereport {{/str}}
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#users}}
                                    <tr>
                                        <td>{{firstname}} {{lastname}}</td>
                                        <td>{{{status}}}</td>
                                        <td>{{reason}}</td>
                                    </tr>
                                    {{/users}}
                                    {{^users}}
                                    <tr>
                                        <td class="text-center" colspan="3">
                                            {{#str}} nosignups, report_elucidsitereport{{/str}}
                                        </td>
                                    </tr>
                                    {{/users}}
                                </tbody>
                            </table>
                        </div>
                    </td>
                </tr>
                {{/sessions}}
                {{^sessions}}
                <tr>
                    <td class="text-center" colspan="10">
                        {{#str}} nosessions, report_elucidsitereport{{/str}}
                    </td>
                </tr>
                {{/sessions}}
                {{/f2fmodules}}
                {{^f2fmodules}}
                <tr>
                    <td class="text-center" colspan="10">
                        {{#str}} nosessions, report_elucidsitereport{{/str}}
                    </td>
                </tr>
                {{/f2fmodules}}
            </tbody>
        </table>
    </div>
</div>