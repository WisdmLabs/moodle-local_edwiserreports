<div id="wdm-elucidsitereport" class="row m-0">
    {{# blocks }}
        <div class="{{class}} mb-5">
            <div id="{{id}}" data-sesskey="{{sesskey}}">
                <div class="panel m-0">
                    <div class="panel-header d-flex">
                        <div class="panel-title px-0 col-10">
                            <strong>
                                {{{name}}}
                                <a href="javascript:void(0)" data-title="{{info}}" class="small" data-toggle="tooltip" data-trigger="hover" data-placement="bottom">
                                    <i class="fa fa-question-circle"></i>
                                </a>
                            </strong>

                            {{{ filters }}}
                        </div>
                        <div class="col-2 p-0">
                            {{> report_elucidsitereport/blocksettingdropdown }}
                        </div>
                    </div>
                    <div class="panel-body p-0">
                        {{{ blockview }}}
                        <div class="loader w-full text-center">
                            {{# pix }} loader, report_elucidsitereport{{/ pix }}
                        </div>
                    </div>
                    <div class="panel-footer row m-0">
                        {{# morelink }}
                            <div class="">
                                <a href="{{{.}}}" class="btn btn-primary btn-sm">{{#str}} more, report_elucidsitereport {{/str}}</a>
                            </div>
                        {{/ morelink }}
                        {{# hasdownloadlink }}
                            <div class="ml-auto row m-0">
                                <a href="{{{.}}}" class="fi fi-csv">
                                    <div class="fi-content">
                                        <strong>csv</strong>
                                    </div>
                                </a>
                                <a href="{{{.}}}" class="fi fi-excel">
                                    <div class="fi-content">
                                        <strong>excel</strong>
                                    </div>
                                </a>
                                <a href="{{{.}}}" class="fi fi-pdf">
                                    <div class="fi-content">
                                        <strong>pdf</strong>
                                    </div>
                                </a>
                                <a href="{{{.}}}" class="fi fi-email">
                                    <div class="fi-content">
                                        <strong>email</strong>
                                    </div>
                                </a>
                            </div>
                        {{/ hasdownloadlink }}
                    </div>
                </div>
            </div>
        </div>
    {{/ blocks }}
</div>

<div class="d-none">
    {{! If user is admin }}
    {{^isreportingmanager}}
    <div class="col-12 px-0">
        {{> report_elucidsitereport/block_activeusers }}
    </div>

    <div class="col-lg-4 col-md-12 col-sm-12">
        {{> report_elucidsitereport/block_courseprogress }}
    </div>

    <div class="col-lg-8 col-md-12 col-sm-12 px-0">
        {{> report_elucidsitereport/block_activecourses }}
    </div>

    {{#hascustomcertpluign}}
    <div class="col-lg-6 col-md-12 col-sm-12 px-0 pr-lg-10">
        {{> report_elucidsitereport/block_certificatestats }}
    </div>
    {{/hascustomcertpluign}}

    <div class="col-lg-6 col-md-12 col-sm-12 px-0 pl-lg-10">
        {{> report_elucidsitereport/block_liveusers }}
    </div>

    {{#hasf2fpluign}}
    <div class="col-12 px-0">
        {{> report_elucidsitereport/block_f2fsessions }}
    </div>
    {{/hasf2fpluign}}

    <div class="col-lg-8 col-md-12 col-sm-12 px-0">
        {{> report_elucidsitereport/block_accessinfo }}
    </div>

    {{#haslppluign}}
    <div class="col-lg-4 col-md-12 col-sm-12 px-0">
        {{> report_elucidsitereport/block_lpstats }}
    </div>
    {{/haslppluign}}

    <div class="col-lg-6 col-md-12 col-sm-12 pl-0 pr-lg-10">
        {{> report_elucidsitereport/block_todaysactivity }}
    </div>

    <div class="col-lg-6 col-md-12 col-sm-12 pr-0 pl-lg-10">
        {{> report_elucidsitereport/block_inactiveusers }}
    </div>

    <!-- <div class="col-12 px-0">
        {{> report_elucidsitereport/block_customreport }}
    </div> -->
    <div class="col-12 px-0">
        {{> report_elucidsitereport/block_customqueryreport }}
    </div>
    {{/isreportingmanager}}


    {{! If user is reporting manager}}
    {{#isreportingmanager}}
        {{#activeusers}}
            <div class="col-12 px-0">
                {{> report_elucidsitereport/block_activeusers }}
            </div>
        {{/activeusers}}

        {{#courseprogress}}
            <div class="col-lg-4 col-md-12 col-sm-12">
                {{> report_elucidsitereport/block_courseprogress }}
            </div>
        {{/courseprogress}}

        {{#activecourses}}
            <div class="col-lg-8 col-md-12 col-sm-12 px-0">
                {{> report_elucidsitereport/block_activecourses }}
            </div>
        {{/activecourses}}

        {{#certificatestats}}
            {{#hascustomcertpluign}}
            <div class="col-lg-6 col-md-12 col-sm-12 px-0 pr-lg-10">
                {{> report_elucidsitereport/block_certificatestats }}
            </div>
            {{/hascustomcertpluign}}
        {{/certificatestats}}

        {{#realtimeusers}}
            <div class="col-lg-6 col-md-12 col-sm-12 px-0 pl-lg-10">
                {{> report_elucidsitereport/block_liveusers }}
            </div>
        {{/realtimeusers}}

        {{#f2fsessions}}
            {{#hasf2fpluign}}
            <div class="col-12 px-0">
                {{> report_elucidsitereport/block_f2fsessions }}
            </div>
            {{/hasf2fpluign}}
        {{/f2fsessions}}

        {{#accessinfo}}
            <div class="col-lg-8 col-md-12 col-sm-12 px-0">
                {{> report_elucidsitereport/block_accessinfo }}
            </div>
        {{/accessinfo}}

        {{#lpstats}}
            {{#haslppluign}}
            <div class="col-lg-4 col-md-12 col-sm-12 px-0">
                {{> report_elucidsitereport/block_lpstats }}
            </div>
            {{/haslppluign}}
        {{/lpstats}}

        {{#todaysactivity}}
            <div class="col-lg-6 col-md-12 col-sm-12 pl-0 pr-lg-10">
                {{> report_elucidsitereport/block_todaysactivity }}
            </div>
        {{/todaysactivity}}

        {{#inactiveusers}}
            <div class="col-lg-6 col-md-12 col-sm-12 pr-0 pl-lg-10">
                {{> report_elucidsitereport/block_inactiveusers }}
            </div>
        {{/inactiveusers}}
        <!-- <div class="col-12 px-0">
            {{> report_elucidsitereport/block_customreport }}
        </div> -->

        <div class="col-12 px-0">
            {{> report_elucidsitereport/block_customqueryreport }}
        </div>
    {{/isreportingmanager}}
</div>