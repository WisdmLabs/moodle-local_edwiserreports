<div id="wdm-courseengage-individual" class="table-responsive mt-1" data-sesskey="{{sesskey}}">
    <table class="table" style="display: none;">
        <thead>
            <tr>
                <th class="text-nowrap bg-image-none no-sorting-arrow">
                    {{#str}} coursename, local_sitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} enrolments, local_sitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} visits, local_sitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} activitystart, local_sitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} completedhalf, local_sitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} coursecompleted, local_sitereport {{/str}}
                </th>
            </tr>
        </thead>
        <tbody>
            <div class="loader w-full text-center py-100 my-50">
                {{# pix }} loader, local_sitereport{{/ pix }}
            </div>
        </tbody>
  </table>
</div>
