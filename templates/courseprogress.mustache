<div class="table-dropdown">
    {{#exportlink}}
        {{> report_elucidsitereport/exportdropdown }}
    {{/exportlink}}
</div>
<div id="wdm-courseprogress-individual" class="table-responsive" data-sesskey="{{sesskey}}">
    <div class="loader w-full text-center py-100 my-50">
        {{# pix }} loader, report_elucidsitereport{{/ pix }}
    </div>
    <table class="table admintable" style="display: none;">
        <thead>
            <th class="text-nowrap bg-image-none no-sorting-arrow">
                {{#str}} coursename, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap bg-image-none no-sorting-arrow">
                {{#str}} noofenrolled, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap bg-image-none no-sorting-arrow">
                {{#str}} noofcompleted, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap bg-image-none no-sorting-arrow">
                {{#str}} noofcompleted20, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap bg-image-none no-sorting-arrow">
                {{#str}} noofcompleted40, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap bg-image-none no-sorting-arrow">
                {{#str}} noofcompleted60, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap bg-image-none no-sorting-arrow">
                {{#str}} noofcompleted80, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap bg-image-none no-sorting-arrow">
                {{#str}} noofincompleted, report_elucidsitereport {{/str}}
            </th>
        </thead>
        <tbody>
            {{#courseprogress}}
            <tr>
                <td>
                    {{{fullname}}}
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="enrolments" data-minvalue="-1" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{enrolments}}
                    </a>
                </td>
                <td>
                	<a href="javascript:void(0)" class="w-full" data-action="completed" data-minvalue="99" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                		{{completed100}}
                	</a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="completed20" data-minvalue="19" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed20}}
                    </a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="completed40" data-minvalue="39" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed40}}
                    </a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="completed60" data-minvalue="59" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed60}}
                    </a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="completed80" data-minvalue="79" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed80}}
                    </a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="incompleted" data-minvalue="-1" data-maxvalue="20" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed00}}
                    </a>
                </td>
            </tr>
            {{/courseprogress}}
        </tbody>
    </table>
</div>
