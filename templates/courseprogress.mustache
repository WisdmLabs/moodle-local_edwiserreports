<div class="table-dropdown">
    {{#exportlink}}
        {{> report_elucidsitereport/exportdropdown }}
    {{/exportlink}}
</div>
<div id="wdm-courseprogress-individual" class="table-responsive" data-sesskey="{{sesskey}}">
    <table class="table admintable d-none">
        <thead>
            <th class="text-nowrap">
                {{#str}} coursename, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap">
                {{#str}} noofenrolled, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap">
                {{#str}} noofcompleted, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap">
                {{#str}} noofcompleted20, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap">
                {{#str}} noofcompleted40, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap">
                {{#str}} noofcompleted60, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap">
                {{#str}} noofcompleted80, report_elucidsitereport {{/str}}
            </th>
            <th class="text-nowrap">
                {{#str}} noofincompleted, report_elucidsitereport {{/str}}
            </th>
        </thead>
        <tbody>
            {{#courseprogress}}
            <tr>
                <td>
                    {{{fullname}}}
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="enrolments" data-minvalue="0" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{enrolments}}
                    </a>
                </td>
                <td>
                	<a href="javascript:void(0)" class="w-full" data-action="completed" data-minvalue="100" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                		{{completed100}}
                	</a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="completed20" data-minvalue="20" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed20}}
                    </a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="completed40" data-minvalue="40" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed40}}
                    </a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="completed60" data-minvalue="60" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed60}}
                    </a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="completed80" data-minvalue="80" data-maxvalue="100" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed80}}
                    </a>
                </td>
                <td>
                    <a href="javascript:void(0)" class="w-full" data-action="incompleted" data-minvalue="0" data-maxvalue="20" data-courseid="{{id}}" data-coursename="{{fullname}}">
                        {{completed00}}
                    </a>
                </td>
            </tr>
            {{/courseprogress}}
            {{^courseprogress}}
            <div class="loader w-full text-center pt-100 mt-50">
                {{# pix }} loader, report_elucidsitereport{{/ pix }}
            </div>
            {{/courseprogress}}
        </tbody>
    </table>
</div>
