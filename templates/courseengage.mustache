<div class="table-dropdown">
    {{#exportlink}}
    {{#courseengage}}
        {{> report_elucidsitereport/exportdropdown }}
    {{/courseengage}}
    {{/exportlink}}
</div>
<div id="wdm-courseengage-individual" class="table-responsive" data-sesskey="{{sesskey}}">
    <table class="table" style="display: none;">
        <thead>
            <tr>
                <th class="text-nowrap bg-image-none no-sorting-arrow">
                    {{#str}} coursename, report_elucidsitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} enrolments, report_elucidsitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} visits, report_elucidsitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} activitystart, report_elucidsitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} completedhalf, report_elucidsitereport {{/str}}
                </th>
                <th class="text-center bg-image-none no-sorting-arrow text-nowrap">
                    {{#str}} coursecompleted, report_elucidsitereport {{/str}}
                </th>
            </tr>
        </thead>
        <tbody>
            <div class="loader w-full text-center py-100 my-50">
                {{# pix }} loader, report_elucidsitereport{{/ pix }}
            </div>
        </tbody>
  </table>
</div>